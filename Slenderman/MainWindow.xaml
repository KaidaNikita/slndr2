        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Slenderman"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Canvas Background="#FF2E0906"  Name="canvas">
		<Rectangle Fill="LemonChiffon" Height="22" Canvas.Left="184" Stroke="Black" StrokeThickness="1" Canvas.Top="40" Width="433"/>
		<Rectangle Fill="LemonChiffon" Height="22" Canvas.Left="184" Stroke="Black" StrokeThickness="1" Canvas.Top="264" Width="433"/>
		<Rectangle Fill="LemonChiffon" Height="26" Canvas.Left="480" Stroke="Black" StrokeThickness="1" Canvas.Top="150" Width="246" RenderTransformOrigin="0.5,0.5">
			<Rectangle.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform Angle="-89.892"/>
					<TranslateTransform/>
				</TransformGroup>
			</Rectangle.RenderTransform>
		</Rectangle>
		<Rectangle Fill="LemonChiffon" Height="22" Canvas.Left="247" Stroke="Black" StrokeThickness="1" Canvas.Top="172" Width="205" RenderTransformOrigin="0.5,0.5">
			<Rectangle.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform Angle="90.358"/>
					<TranslateTransform/>
				</TransformGroup>
			</Rectangle.RenderTransform>
		</Rectangle>
